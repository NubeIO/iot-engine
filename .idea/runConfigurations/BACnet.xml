<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="BACnet" type="Application" factoryName="Application">
    <option name="INCLUDE_PROVIDED_SCOPE" value="true" />
    <option name="MAIN_CLASS_NAME" value="io.github.zero88.qwe.CarlLauncher" />
    <module name="iot-engine.services.bacnet.main" />
    <option name="PROGRAM_PARAMETERS" value="run com.nubeiot.edge.connector.bacnet.BACnetVerticle" />
    <option name="WORKING_DIRECTORY" value="$MODULE_DIR$" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.nubeiot.edge.connector.bacnet.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <extension name="net.ashald.envfile">
      <option name="IS_ENABLED" value="false" />
      <option name="IS_SUBST" value="false" />
      <option name="IS_PATH_MACRO_SUPPORTED" value="false" />
      <option name="IS_IGNORE_MISSING_FILES" value="false" />
      <option name="IS_ENABLE_EXPERIMENTAL_INTEGRATIONS" value="false" />
      <ENTRIES>
        <ENTRY IS_ENABLED="true" PARSER="runconfig" />
      </ENTRIES>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>
