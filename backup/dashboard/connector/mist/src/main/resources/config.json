{
  "__app__": {
    "__micro__": {
      "__gateway__": {
        "enabled": false
      },
      "__serviceDiscovery__": {
        "enabled": true
      },
      "__localServiceDiscovery__": {
        "enabled": false
      },
      "__circuitBreaker__": {
        "enabled": false
      }
    },
    "__http__": {
      "port": 8080,
      "__rest__": {
        "rootApi": "/mist",
        "__dynamic__": {
          "enabled": false
        }
      }
    },
    "__httpClient__": {
      "handlerConfig": {
        "httpLightBodyHandlerClass": "com.nubeiot.dashboard.connector.mist.HttpMistResponseBodyHandler"
      }
    },
    "__mist__": {
      "schema": "http",
      "host": "mist",
      "port": 2004
    }
  }
}
