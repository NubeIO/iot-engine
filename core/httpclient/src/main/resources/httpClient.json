{
  "userAgent": "nubeio.httpclient",
  "httpHandlerConfig": {
    "clientWriterClass": "com.nubeiot.core.http.client.handler.HttpClientWriter",
    "errorHandlerClass": "com.nubeiot.core.http.client.handler.HttpClientErrorHandler",
    "endHandlerClass": "com.nubeiot.core.http.client.handler.HttpClientEndHandler",
    "lightBodyHandlerClass": "com.nubeiot.core.http.client.handler.HttpLightResponseBodyHandler"
  },
  "websocketOptions": {
    "idleTimeout": -1,
    "maxWebsocketFrameSize": 65536,
    "maxWebsocketMessageSize": 262144,
    "websocketCompressionLevel": 6,
    "websocketCompressionAllowClientNoContext": true,
    "websocketCompressionRequestServerNoContext": true
  },
  "options": {
    "sendBufferSize": -1,
    "receiveBufferSize": -1,
    "trafficClass": -1,
    "reuseAddress": true,
    "logActivity": false,
    "reusePort": false,
    "tcpNoDelay": true,
    "tcpKeepAlive": false,
    "soLinger": -1,
    "usePooledBuffers": false,
    "idleTimeout": 30,
    "idleTimeoutUnit": "SECONDS",
    "ssl": false,
    "enabledCipherSuites": [],
    "crlPaths": [],
    "crlValues": [],
    "useAlpn": false,
    "enabledSecureTransportProtocols": [
      "TLSv1",
      "TLSv1.1",
      "TLSv1.2"
    ],
    "tcpFastOpen": false,
    "tcpCork": false,
    "tcpQuickAck": false,
    "connectTimeout": 60000,
    "trustAll": false,
    "metricsName": "",
    "verifyHost": true,
    "maxPoolSize": 5,
    "keepAlive": true,
    "keepAliveTimeout": 60,
    "pipeliningLimit": 10,
    "pipelining": false,
    "http2MaxPoolSize": 1,
    "http2MultiplexingLimit": -1,
    "http2ConnectionWindowSize": -1,
    "http2KeepAliveTimeout": 60,
    "poolCleanerPeriod": 1000,
    "tryUseCompression": true,
    "protocolVersion": "HTTP_1_1",
    "maxChunkSize": 8192,
    "maxInitialLineLength": 4096,
    "maxHeaderSize": 8192,
    "maxWaitQueueSize": -1,
    "initialSettings": {
      "headerTableSize": 4096,
      "pushEnabled": true,
      "maxConcurrentStreams": 4294967295,
      "initialWindowSize": 65535,
      "maxFrameSize": 16384,
      "maxHeaderListSize": 2147483647
    },
    "alpnVersions": [],
    "http2ClearTextUpgrade": true,
    "sendUnmaskedFrames": false,
    "maxRedirects": 16,
    "forceSni": false,
    "decoderInitialBufferSize": 128
  }
}
