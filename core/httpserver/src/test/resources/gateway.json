{
  "__app__": {
    "__micro__": {
      "__gateway__": {
        "enabled": true,
        "localAnnounceMonitorClass": "com.nubeiot.core.http.gateway.RouteGatewayAnnounceHandler"
      },
      "__serviceDiscovery__": {
        "enabled": false
      },
      "__localServiceDiscovery__": {
        "enabled": true
      },
      "__circuitBreaker__": {
        "enabled": false
      }
    },
    "__http__": {
      "__dynamic__": {
        "enabled": true,
        "path": "/s"
      }
    }
  }
}
