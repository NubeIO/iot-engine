{
  "__app__": {
    "__http__": {
      "host": "0.0.0.0",
      "port": 9999,
      "__rest__": {
        "enabled": false
      },
      "__files__": {
        "enabled": true,
        "dir": "files",
        "__upload__": {
          "enabled": true,
          "path": "/u",
          "maxBodySizeMB": 1,
          "handlerClass": "com.nubeiot.core.http.handler.UploadFileHandler",
          "listenerAddress": "blank.mean.using.container.class.name.upload",
          "listenerClass": "com.nubeiot.core.http.handler.UploadListener"
        },
        "__download__": {
          "enabled": true,
          "path": "/f"
        }
      }
    }
  }
}
