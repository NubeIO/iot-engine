{
  "__app__": {
    "__micro__": {
      "__serviceDiscovery__": {
        "announceAddress": "vertx.discovery.announce",
        "autoRegistrationOfImporters": true,
        "backendConfiguration": {},
        "usageAddress": "vertx.discovery.usage"
      },
      "__circuitBreaker__": {
        "name": "nubeio-circuit-breaker",
        "options": {
          "timeout": 10000,
          "maxFailures": 5,
          "fallbackOnFailure": false,
          "resetTimeout": 30000,
          "notificationAddress": "vertx.circuit-breaker",
          "notificationPeriod": 2000,
          "maxRetries": 0,
          "metricsRollingWindow": 10000,
          "metricsRollingBuckets": 10,
          "failuresRollingWindow": 10000
        }
      }
    }
  }
}
