{
  "dataDir": "/data",
  "__system__": {
    "__eventBus__": {
      "clientAuth": "NONE",
      "ssl": false,
      "clustered": false
    },
    "__cluster__": {
      "active": false
    }
  },
  "__app__": {
    "__micro__": {
      "__gateway__": {
        "enabled": false
      },
      "__serviceDiscovery__": {
        "enabled": false
      },
      "__localServiceDiscovery__": {
        "enabled": true
      },
      "__circuitBreaker__": {
        "enabled": false
      }
    },
    "__http__": {
      "port": 8000,
      "publicScheme": "HTTP",
      "__static__": {
        "enabled": true,
        "webPath": ""
      }
    },
    "__sql__": {
      "dialect": "H2",
      "__hikari__": {
        "jdbcUrl": "jdbc:h2:file:./edge-dashboard",
        "minimumIdle": 1,
        "maximumPoolSize": 2,
        "connectionTimeout": 30000,
        "idleTimeout": 180000,
        "maxLifetime": 300000
      }
    },
    "__edge_dashboard_connection__": {}
  }
}
