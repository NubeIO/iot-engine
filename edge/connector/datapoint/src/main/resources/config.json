{
  "__app__": {
    "__micro__": {
      "__gateway__": {
        "enabled": false
      },
      "__serviceDiscovery__": {
        "enabled": false
      },
      "__localServiceDiscovery__": {
        "enabled": true
      },
      "__circuitBreaker__": {
        "enabled": false
      }
    },
    "__sql__": {
      "dialect": "H2",
      "dbName": "datapoint",
      "dbLocalFile": true,
      "__hikari__": {
        "minimumIdle": 1,
        "maximumPoolSize": 2,
        "connectionTimeout": 30000,
        "idleTimeout": 180000,
        "maxLifetime": 300000
      }
    }
  }
}

