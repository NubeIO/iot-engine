{
  "__system__": {
    "__eventBus__": {
      "ssl": true,
      "clustered": false,
      "keyStoreOptions": {
        "path": "eventBusKeystore.jks",
        "password": "nubesparkEventBus"
      },
      "trustStoreOptions": {
        "path": "eventBusKeystore.jks",
        "password": "nubesparkEventBus"
      }
    },
    "__cluster__": {
      "active": false
    }
  },
  "__app__": {
    "__micro__": {
      "__gateway__": {
        "enabled": true,
        "localAnnounceMonitorClass": "com.nubeiot.core.http.gateway.RouterAnnounceListener"
      },
      "__serviceDiscovery__": {
        "enabled": false
      },
      "__localServiceDiscovery__": {
        "enabled": true
      },
      "__circuitBreaker__": {
        "enabled": true
      }
    },
    "__http__": {
      "__dynamic__": {
        "enabled": true,
        "path": "/driver"
      },
      "host": "0.0.0.0",
      "port": 8091,
      "enabled": true,
      "rootApi": "/api"
    },
    "api.name": "driver-api"
  }
}
