{
  "__app__": {
    "__micro__": {
      "__gateway__": {
        "enabled": true,
        "localAnnounceMonitorClass": "com.nubeiot.core.http.gateway.RouterAnnounceListener"
      },
      "__serviceDiscovery__": {
        "enabled": false
      },
      "__localServiceDiscovery__": {
        "enabled": true
      },
      "__circuitBreaker__": {
        "enabled": true
      }
    },
    "__http__": {
      "__rest__": {
        "__dynamic__": {
          "enabled": true,
          "path": "/driver"
        }
      }
    }
  }
}
