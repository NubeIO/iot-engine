{
  "__system__": {
    "__eventBus__": {
      "clientAuth": "NONE",
      "ssl": false,
      "clustered": false
    },
    "__cluster__": {
      "active": false
    }
  },
  "__app__": {
    "__micro__": {
      "__gateway__": {
        "enabled": true,
        "localAnnounceMonitorClass": "com.nubeiot.core.http.gateway.RouterAnnounceListener"
      },
      "__serviceDiscovery__": {
        "enabled": false
      },
      "__localServiceDiscovery__": {
        "enabled": true
      },
      "__circuitBreaker__": {
        "enabled": true,
        "options": {
          "timeout": 60000
        }
      }
    },
    "__http__": {
      "port": 8080,
      "__rest__": {
        "rootApi": "/api",
        "__dynamic__": {
          "enabled": true,
          "path": ""
        }
      },
      "__cors__": {
        "allowedHeaders": [
          "Access-Control-Request-Method",
          "Access-Control-Allow-Credentials",
          "Access-Control-Allow-Origin",
          "Access-Control-Allow-Headers",
          "Content-Type",
          "origin",
          "x-requested-with",
          "accept",
          "Authorization"
        ]
      }
    }
  }
}
