{
  "dataDir": "/data",
  "__system__": {
    "__eventBus__": {
      "ssl": true,
      "keyStoreOptions": {
        "path": "eventBusKeystore.jks",
        "password": "nubesparkEventBus"
      },
      "trustStoreOptions": {
        "path": "eventBusKeystore.jks",
        "password": "nubesparkEventBus"
      }
    },
    "__cluster__": {
      "active": true,
      "ha": true
    }
  },
  "__app__": {
    "__micro__": {
      "__gateway__": {
        "enabled": false
      },
      "__serviceDiscovery__": {
        "enabled": true
      },
      "__localServiceDiscovery__": {
        "enabled": false
      },
      "__circuitBreaker__": {
        "enabled": false
      }
    },
    "__http__": {
      "port": 8080,
      "rootApi": "/api/sql-pg",
      "__dynamic__": {
        "enabled": true
      }
    },
    "__pg__": {
      "host": "pg-db",
      "port": 5432,
      "maxPoolSize": 10,
      "username": "sandbox",
      "password": "111",
      "database": "sandbox_db",
      "charset": "UTF-8",
      "queryTimeout": 10000
    }
  }
}
