{
  "dataDir": "/data",
  "__system__": {
    "__eventBus__": {
      "ssl": true,
      "keyStoreOptions": {
        "path": "eventBusKeystore.jks",
        "password": "nubesparkEventBus"
      },
      "trustStoreOptions": {
        "path": "eventBusKeystore.jks",
        "password": "nubesparkEventBus"
      }
    },
    "__cluster__": {
      "active": true,
      "ha": true
    }
  },
  "__app__": {
    "__micro__": {
      "__gateway__": {
        "enabled": false
      },
      "__serviceDiscovery__": {
        "enabled": true
      },
      "__localServiceDiscovery__": {
        "enabled": false
      },
      "__circuitBreaker__": {
        "enabled": false
      }
    },
    "__http__": {
      "port": 8087,
      "__rest__": {
        "rootApi": "/zeppelin",
        "__dynamic__": {
          "enabled": false
        }
      }
    },
    "__zeppelin__": {
      "server.host": "localhost",
      "server.port": 18080
    }
  }
}
